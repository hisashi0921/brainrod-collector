{
  "manifest_version": 3,
  "name": "ブレインロッド オートクリッカー",
  "version": "1.0",
  "description": "ブレインロッドゲームで一番安いロッドを自動でクリックします",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
